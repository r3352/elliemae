<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Elli.Domain" namespace="Elli.Domain.Mortgage">
  <class name="ShippingContact" table="ElliShippingContact" dynamic-update="true">
    <id name="Id" column="ElliShippingContactId" unsaved-value="00000000-0000-0000-0000-000000000000">
      <generator class="guid.comb" />
    </id>
    <version name="Version" />
    <many-to-one name="Shipping" column="ElliShippingId" />
    <property name="ShippingContactType" />
    <property name="Address" />
    <property name="City" />
    <property name="ContactName" />
    <property name="Email" />
    <property name="Fax" />
    <property name="Name" />
    <property name="Phone" />
    <property name="PostalCode" />
    <property name="State" />
    <property name="Address2" />
  </class>
</hibernate-mapping>

